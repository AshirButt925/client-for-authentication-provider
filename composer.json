{
    "name": "client-for-authentication-provider/client-for-authentication-provider",
    "description": "Laravel package for client authentication provider with mini app login functionality",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashir Ali Butt",
            "email": "ashirbutt925@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^7.1|^7.2|^7.3",
        "laravel/framework": "^5.3",
        "laravel/passport": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "ClientForAuthenticationProvider\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClientForAuthenticationProvider\\ClientForAuthenticationProviderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "authentication",
        "client",
        "mini-app",
        "login",
        "sanctum"
    ],
    "homepage": "https://github.com/ashirbutt925/client-for-authentication-provider",
    "support": {
        "issues": "https://github.com/ashirbutt925/client-for-authentication-provider/issues",
        "source": "https://github.com/ashirbutt925/client-for-authentication-provider"
    }
}
